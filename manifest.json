{
    "manifest_version": 2,
    "short_name": "Unipos_ReactJs",
    "name": "Unipos Using ReactJs",
    "version": "1.1",
    "permissions": [],
    "background": {
    },
    "content_scripts": [
        {
        "matches": ["https://unipos.me/*"],
        "js": ["public/js/app.js"]
        }
    ],
    "browser_action": {
        "default_popup": "public/index.html",
        "default_title": "React Ext"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
